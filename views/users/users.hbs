{{!-- List of all users --}}
<h1>Keepers list</h1>
{{#if users}}
{{#each users}}
<div class="image-wrap-center">
    <div class="mage-wrap-center"></div>
        <img class="img-rounded " src="{{this.img}}" width="18%" height="18%">
    </div>
</div>
<p>Name: {{this.name}}</p>
<p>Last name: {{this.lastName}}</p>
<p>email: {{this.email}}</p>
<button class="button-grey"><a href="users/{{this._id}}">Details</a></button>
{{/each}}

{{/if}}
<button class="button-blue"><a href="/profile">My profile</a></button>